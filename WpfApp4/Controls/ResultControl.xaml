<UserControl x:Class="WpfApp4.Controls.ResultControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfApp4.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <UniformGrid HorizontalAlignment="Center" VerticalAlignment="Center" Columns="2">
            <TextBlock Text="Количество правильных ответов:" Margin="0,0,10,0"/>
            <TextBlock Name="Cor"/>
            <TextBlock Text="Количество не правильных ответов:" Margin="0,0,10,0"/>
            <TextBlock Name="InCor"/>
            <TextBlock Text="Всего вопросов:" Margin="0,0,10,0"/>
            <TextBlock Name="Total"/>
        </UniformGrid>
    </Grid>
</UserControl>
